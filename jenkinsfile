pipeline {
     
     agent any
     tools { 
        maven 'Maven 3.9.1' 
        
     }
     stages{
     
         stage('Git Checkout'){

            steps{
               git branch:'main', url:'https://github.com/Manjunath72/demo-counter-app.git'
            }
         }
         stage('UNIT Testing'){

            steps{
               sh 'mvn test'
            }
         }
     }
}

